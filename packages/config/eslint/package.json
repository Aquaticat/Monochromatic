{
  "name": "@monochromatic-dev/config-eslint",
  "keywords": [
    "eslint",
    "config",
    "monochromatic"
  ],
  "dependencies": {
    "typescript-eslint": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "eslint-plugin-jsdoc": "catalog:",
    "@eslint/js": "catalog:",
    "eslint-plugin-n": "catalog:",
    "@vitest/eslint-plugin": "catalog:",
    "eslint-plugin-oxlint": "catalog:",
    "@typescript-eslint/scope-manager": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@typescript-eslint/types": "catalog:",
    "@typescript-eslint/utils": "catalog:"
  },
  "peerDependencies": {
    "eslint": "catalog:",
    "vite": "catalog:"
  },
  "devDependencies": {
    "@monochromatic-dev/config-typescript": "workspace:*",
    "@monochromatic-dev/config-vite": "workspace:*",
    "@types/bun": "catalog:",
    "@vitest/browser": "catalog:"
  },
  "type": "module",
  "files": [
    "src",
    "dist/final"
  ],
  "author": {
    "name": "Aquaticat",
    "email": "an@aquati.cat",
    "url": "https://aquati.cat"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aquaticat/Monochromatic.git",
    "directory": "packages/config/eslint"
  },
  "exports": {
    ".": {
      "types": "./dist/final/types/src/index.d.ts",
      "default": "./dist/final/js/index.js"
    },
    "./.ts": {
      "default": "./src/index.ts"
    },
    "./.js": {
      "types": "./dist/final/types/src/index.d.ts",
      "default": "./dist/final/js/index.js"
    }
  }
}
