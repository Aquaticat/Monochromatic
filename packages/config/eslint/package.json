{
  "name": "@monochromatic-dev/config-eslint",
  "keywords": [
    "eslint",
    "config",
    "monochromatic"
  ],
  "dependencies": {
    "typescript-eslint": "catalog:"
  },
  "peerDependencies": {
    "eslint": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "eslint-plugin-oxlint": "catalog:",
    "vite": "catalog:",
    "@monochromatic-dev/config-typescript": "*",
    "@monochromatic-dev/config-vite": "*",
    "@monochromatic-dev/config-oxlint": "*",
    "@types/bun": "catalog:",
    "@vitest/browser": "catalog:"
  },
  "type": "module",
  "files": [
    "src",
    "dist/final"
  ],
  "author": {
    "name": "Aquaticat",
    "email": "an@aquati.cat",
    "url": "https://aquati.cat"
  },
  "scripts": {
    "w:types": "tsc --watch",
    "b:types": "tsc",
    "w:js": "vite build --config vite.config.ts  --watch",
    "b:js": "vite build  --config vite.config.ts ",
    "b": "pnpm run --stream '/^b:\\w+$/'",
    "w": "pnpm run --stream '/^w:\\w+$/'"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aquaticat/Monochromatic.git",
    "directory": "packages/config/eslint"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./.ts": {
      "default": "./src/index.ts"
    },
    "./.js": {
      "types": "./dist/final/types/src/index.d.ts",
      "default": "./dist/final/js/index.js"
    }
  }
}
