{
  "name": "@monochromatic-dev/module-es",
  "description": "Common utils for functional programming.",
  "type": "module",
  "module": "dist/final/index.js",
  "exports": {
    ".": {
      "types": "./dist/final/types/src/index.d.ts",
      "node": "./dist/final/js/index.node.js",
      "default": "./dist/final/js/index.js"
    },
    "./ts": {
      "default": "./src/index.ts"
    },
    "./.ts": {
      "default": "./src/index.ts"
    },
    "./.js": {
      "types": "./dist/final/types/src/index.d.ts",
      "node": "./dist/final/js/index.node.js",
      "default": "./dist/final/js/index.js"
    },
    "./logtape.ts": {
      "node": "./src/logtape.node.ts",
      "default": "./src/logtape.default.ts"
    },
    "./fs.ts": {
      "node": "./src/fs.node.ts",
      "default": "./src/fs.default.ts"
    },
    "./fs.fs.ts": {
      "node": "./src/fs.fs.node.ts",
      "default": "./src/fs.fs.default.ts"
    },
    "./fs.pathJoin.ts": {
      "node": "./src/fs.pathJoin.node.ts",
      "default": "./src/fs.pathJoin.default.ts"
    },
    "./fs.pathParse.ts": {
      "node": "./src/fs.pathParse.node.ts",
      "default": "./src/fs.pathParse.default.ts"
    },
    "./string.fs.ts": {
      "node": "./src/string.fs.node.ts",
      "default": "./src/string.fs.default.ts"
    }
  },
  "bin": {
    "checkBuild": "./src/moon.checkBuild.ts",
    "checkDependencies": "./src/moon.checkDependencies.ts",
    "checkGitHooks": "./src/moon.checkGitHooks.ts",
    "checkTools": "./src/moon.checkTools.ts",
    "bunCompile": "./src/moon.bunCompile.ts",
    "pnpmInstall": "./src/moon.pnpmInstall.ts",
    "preparePlaywright": "./src/moon.preparePlaywright.ts",
    "append": "./src/cli.append.ts",
    "command": "./src/cli.command.ts"
  },
  "files": [
    "dist/final",
    "src"
  ],
  "devDependencies": {
    "@monochromatic-dev/config-typescript": "workspace:*",
    "@monochromatic-dev/module-es": "workspace:*",
    "@monochromatic-dev/config-vite": "workspace:*",
    "type-fest": "catalog:",
    "@types/bun": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@logtape/logtape": "catalog:",
    "@logtape/file": "catalog:",
    "serialize-error": "catalog:",
    "lfi": "catalog:",
    "@kazupon/gunshi": "catalog:",
    "nano-spawn": "catalog:",
    "@total-typescript/ts-reset": "catalog:",
    "ts-pattern": "catalog:",
    "happy-opfs": "catalog:",
    "@vitest/browser-playwright": "catalog:",
    "happy-rusty": "catalog:",
    "@csstools/css-tokenizer": "catalog:",
    "tinyglobby": "catalog:",
    "find-up": "catalog:",
    "@cspotcode/outdent": "catalog:",
    "superjson": "catalog:",
    "decircular": "catalog:",
    "safe-stringify": "catalog:"
  },
  "dependencies": {
    "meilisearch": "catalog:",
    "zod": "catalog:"
  },
  "version": "0.0.125",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aquaticat/Monochromatic.git",
    "directory": "module/es"
  },
  "sideEffects": false
}
