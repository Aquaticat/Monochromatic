{
  "name": "@monochromatic-dev/module-es",
  "description": "Common utils for functional programming.",
  "type": "module",
  "module": "dist/final/index.js",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./ts": {
      "default": "./src/index.ts"
    },
    "./.ts": {
      "default": "./src/index.ts"
    },
    "./.js": {
      "types": "./dist/final/types/src/index.d.ts",
      "default": "./dist/final/js/index.js"
    },
    "./logtape.ts": {
      "node": "./src/logtape.node.ts",
      "default": "./src/logtape.default.ts"
    },
    "./fs.ts": {
      "node": "./src/fs.node.ts",
      "default": "./src/fs.default.ts"
    },
    "./fs.fs.ts": {
      "node": "./src/fs.fs.node.ts",
      "default": "./src/fs.fs.default.ts"
    }
  },
  "scripts": {
    "w:types": "tsc --watch",
    "b:types": "tsc",
    "b:js_node": "vite build --mode production,node",
    "w:js_node": "vite build --mode production,node --watch",
    "b:js_browser": "vite build --mode production,browser",
    "w:js_browser": "vite build --mode production,browser --watch",
    "b": "pnpm run --stream '/^b:\\w+$/'",
    "w": "pnpm run --stream '/^w:\\w+$/'"
  },
  "files": [
    "dist/final",
    "src"
  ],
  "devDependencies": {
    "@monochromatic-dev/config-typescript": "workspace:*",
    "@monochromatic-dev/module-es": "workspace:*",
    "type-fest": "catalog:",
    "@types/bun": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@logtape/logtape": "catalog:",
    "@logtape/file": "catalog:",
    "serialize-error": "catalog:",
    "lfi": "catalog:",
    "nano-spawn": "catalog:",
    "@total-typescript/ts-reset": "catalog:",
    "ts-pattern": "catalog:",
    "happy-opfs": "catalog:"
  },
  "dependencies": {
  },
  "version": "0.0.125",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aquaticat/Monochromatic.git",
    "directory": "module/es"
  },
  "sideEffects": false
}
