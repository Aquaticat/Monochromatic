{
  "name": "@monochromatic-dev/module-es",
  "description": "Common utils for functional programming.",
  "type": "module",
  "module": "dist/final/index.js",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./ts": {
      "default": "./src/index.ts"
    },
    "./.ts": {
      "default": "./src/index.ts"
    },
    "./.js": {
      "types": "./dist/final/types/src/index.d.ts",
      "default": "./dist/final/index.js"
    },
    "./logtape.ts": {
      "node": "./src/logtape.node.ts",
      "default": "./src/logtape.default.ts"
    }
  },
  "scripts": {
    "vite": "vite",
    "wd": "bun run watch-development.ts",
    "w:t": "tsc --watch"
  },
  "files": [
    "dist/final",
    "src"
  ],
  "devDependencies": {
    "@monochromatic-dev/config-typescript": "workspace:*",
    "type-fest": "catalog:",
    "@types/bun": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@logtape/logtape": "catalog:",
    "@logtape/file": "catalog:",
    "serialize-error": "catalog:",
    "lfi": "catalog:",
    "nano-spawn": "catalog:"
  },
  "dependencies": {
  },
  "version": "0.0.125",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aquaticat/Monochromatic.git",
    "directory": "module/es"
  },
  "sideEffects": false
}
